<script>

$(document).ready(function(){
   $(window).resize(function(){
	   jieri_width();	   
	});
	
	jieri_width();
	
	function jieri_width(){
		// 图片的宽
	   var tw = 1600; 
	   // 窗口的宽
	   var wh = $(window).width();
	  
	   // 小于1300的屏幕不在缩放
	   if(wh <1400){
		    $('.main_big').css('width',1400);
			var offceWidth = ((tw - 1400) / 2) - (tw - 1400);
			$('#kinMaxShow').css("left",offceWidth);
			return false;
	   }else{
		    // 设置
		   $('.main_big').css('width',wh);
		   
		   if (wh < tw){
				var offceWidth = ((tw - wh) / 2) - (tw - wh);
				$('#kinMaxShow').css("left",offceWidth);
				$('#pay-position').css('right',237-(tw-wh))
		   }
	   }
	  
	}
	
	// 数字加减
	$('.spinner').spinner({value:1});

	$(document).delegate('.dg-enter', 'click', function(){		
		window.location.href="http://www.huayingcul.com/jieri_order.php?act=index&goods_id=1597&number="+$('#number').val();
	});
		
	
	
});

</script>
