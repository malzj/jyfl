<script>

$(document).ready(function(){
   $(window).resize(function(){
	   jieri_width();	   
	});
	
	jieri_width();
	
	function jieri_width(){
		// 图片的宽
	   var tw = 1920; 
	   // 窗口的宽
	   var wh = $(window).width();
	  
	   // 小于1300的屏幕不在缩放
	   if(wh <1300){
		    $('.main_big').css('width',1300);
			var offceWidth = ((tw - 1300) / 2) - (tw - 1300);
			$('#kinMaxShow').css("left",offceWidth);
			return false;
	   }else{
		    // 设置
		   $('.main_big').css('width',wh);
		   
		   if (wh < tw){
				var offceWidth = ((tw - wh) / 2) - (tw - wh);
				$('#kinMaxShow').css("left",offceWidth);
		   }
	   }
	  
	}
});

</script>
